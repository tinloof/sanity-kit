{
	"name": "@tinloof/sanity-media",
	"version": "0.0.0-20260210151319",
	"description": "Media management plugin for Sanity with S3-compatible storage support",
	"license": "MIT",
	"author": "Tinloof",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./package.json": "./package.json"
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": [
		"dist",
		"README.md"
	],
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"typecheck": "tsc --noEmit",
		"prepublishOnly": "npm run typecheck && npm run build"
	},
	"sideEffects": false,
	"keywords": [
		"sanity",
		"sanity-plugin",
		"media",
		"s3",
		"r2",
		"storage"
