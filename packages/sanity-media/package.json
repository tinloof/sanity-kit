	"devDependencies": {
		"@sanity/icons": "^3.7.4",
		"@sanity/ui": "^3.1.11",
		"@types/react": "^19.2.7",
		"@types/react-dom": "^19.2.3",
		"react": "^19.2.1",
		"react-dom": "^19.2.1",
		"sanity": "^4.20.1",
		"styled-components": "^6.1.19",
		"tsup": "^8.5.1",
		"typescript": "^5.9.3"
	},
	"peerDependencies": {
		"@sanity/icons": "^3",
		"@sanity/ui": "^3",
		"react": "^18 || ^19",
		"sanity": "^4.20.0",
		"styled-components": "^6"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.800.0",
		"@aws-sdk/s3-request-presigner": "^3.800.0",
		"@sanity/studio-secrets": "^3.0.3",
		"swr": "^2.3.3"
	},
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/tinloof/sanity-kit.git",
		"directory": "packages/sanity-media"
	},
	"bugs": {
		"url": "https://github.com/tinloof/sanity-kit/issues"
