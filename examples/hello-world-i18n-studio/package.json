{
	"name": "@examples/hello-world-i18n-studio",
	"private": true,
	"version": "0.0.11",
	"main": "package.json",
	"scripts": {
		"dev": "sanity dev",
		"build": "sanity build ../hello-world-i18n-next/public/cms -y",
		"extract-types": "sanity schema extract",
		"typegen:watch": "watch \"npm run typegen\" ./src",
		"typegen": "pnpm run extract-types && sanity typegen generate",
		"lint": "biome lint .",
		"check": "biome check --write ."
	},
	"exports": {
		"./queries": "./src/queries/index.ts",
		"./types": "./sanity.types.ts"
	},
	"dependencies": {
		"@sanity/client": "^7.14.1",
		"@sanity/vision": "^5.7.0",
		"@tinloof/sanity-document-i18n": "workspace:*",
		"@tinloof/sanity-document-options": "workspace:*",
		"@tinloof/sanity-extends": "workspace:*",
		"@tinloof/sanity-studio": "workspace:*",
		"groq": "^5.7.0",
		"react": "^19.2.4",
		"react-dom": "^19.2.4",
		"sanity": "^5.7.0",
		"styled-components": "^6.3.8"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.13",
		"@types/react": "^19.2.10",
		"@types/react-dom": "^19.2.3",
		"typescript": "^5.9.3",
		"watch": "^1.0.2"
	}
}
